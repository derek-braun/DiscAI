                    android:text="Hole 8: "
                    android:textSize="20dp"/>
            </TableRow>
            <TableRow>
                <TextView
                    android:text="Hole 9: "
                    android:textSize="20dp"/>
            </TableRow>
            <TableRow android:layout_marginTop="50dp">
                <TextView
                    android:text="Front 9: "
                    android:textSize="20dp"
                    android:textStyle="bold"/>
            </TableRow>
            <TableRow>
                <TextView
                    android:text="Back 9: "
                    android:textSize="20dp"
                    android:textStyle="bold"/>
            </TableRow>
            <TableRow>
                <TextView
                    android:text="Total: "
                    android:textSize="20dp"
                    android:textStyle="bold"/>
            </TableRow>
        </TableLayout>
        <TableLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="50dp">
